appname = api_gateway_service
version = 1.1.0
serverTimeout=120
executionTimeout=30
httpport = 8082
runmode = dev
autorender = false
copyrequestbody = true
EnableDocs = true
lang="en|id"
logPath="./logs/api.log"
slackWebhookUrlLog = ""
initData=true

[database]
debug=true
driver="postgres"
host="localhost"
username=postgres
password=admin
name=products
port=5432
options="TimeZone=Asia/Jakarta"
maxOpenConn = 25
maxIdleConn = 25k
maxLifeTimeConn = 300
maxIdleTimeConn = 300